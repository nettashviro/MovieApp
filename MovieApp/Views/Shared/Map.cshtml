
@{
    ViewBag.Title = "Map";
}


<script src="https://polyfill.io/v3/polyfill.min.js?features=default"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBAcpASlmqwBs5DpRzYzOo4-Tymi8lnp_o&callback=initMap&libraries=&v=weekly"
        defer></script>
<style type="text/css">
        #map {
            height: 50vw;
        }

        html,
        body {
            height: 100%;
            margin: 0;
            padding: 0;
        }
</style>
<script>
    "use strict";

    let map;

    function initMap() {
    map = new google.maps.Map(document.getElementById("map"), {
        center: {
            lat: 31.771959,
            lng: 35.217018
        },
        zoom: 8
    });
    }
</script>
<div id="map"></div>
